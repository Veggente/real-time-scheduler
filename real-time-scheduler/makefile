all:
	clang++ -Wall -std=c++11 -stdlib=libc++ *.cpp -O3 -o real-time-scheduler
old:
	clang++ -mmacosx-version-min=10.7 -Wall -std=c++11 -stdlib=libc++ *.cpp -O3 -o real-time-scheduler
g++:
	/opt/centos/devtoolset-1.1/root/usr/bin/g++ -Wall -std=c++11 *.cpp -O3 -o real-time-scheduler
clean:
	rm -rf real-time-simulator
